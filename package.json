{
  "name": "tab-master-ts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/chrome": "^0.0.164",
    "@types/ncp": "^2.0.5",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.17",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "concurrently": "^6.4.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "json-loader": "^0.5.7",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.15",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.2",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit --pretty",
    "copy-files": "ncp manifest.json build/manifest.json",
    "watch:manifest": "nodemon scripts/setup.js --verbose --watch manifest.json",
    "watch:backgroundScript": "yarn copy-files && babel src/backgroundScript --watch --copy-files --out-dir build/backgroundScript --extensions .ts,.tsx",
    "watch": "concurrently \"yarn watch:manifest\" \"yarn watch:backgroundScript\"",
    "dev:contentScript": "webpack serve",
    "dev": "concurrently \"yarn dev:contentScript\" \"yarn watch\"",
    "build:backgroundScript": "yarn copy-files && babel src/backgroundScript --copy-files --out-dir build/backgroundScript --extensions .ts,.tsx",
    "build": "NODE_ENV=production webpack && yarn build:backgroundScript"
  },
  "dependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "fuse.js": "^6.4.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-frame-component": "^5.2.1",
    "react-shadow": "^19.0.2",
    "styled-components": "^5.3.3"
  }
}
